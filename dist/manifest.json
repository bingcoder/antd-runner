{
  "name": "Ant Design Runner",
  "description": "Ant Design 本地编辑预览，无需跳转CodeSandbox、CodePen",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://ant.design/*",
        "https://ant-design.antgroup.com/*",
        "https://ant-design.gitee.io/*"
      ],
      "js": ["content-script.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["js/*"],
      "matches": [
        "https://ant.design/*",
        "https://ant-design.antgroup.com/*",
        "https://ant-design.gitee.io/*"
      ]
    }
  ],
  "permissions": ["storage"],
  "options_ui": {
    "page": "option.html"
  }
}
